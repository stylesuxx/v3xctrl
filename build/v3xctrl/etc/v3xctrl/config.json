{
  "autostart": {
      "control": false,
      "video": false
  },
  "host": "192.168.1.100",
  "ports": {
      "control": 6668,
      "video": 6666,
      "webinterface": 5000
  },
  "video": {
      "bitrate": 1800000,
      "framerate": 30,
      "iFramePeriod": 15,
      "height": 720,
      "width": 1280,
      "record": false,
      "recordPath": "/data/recordings",
      "testSource": true
  },
  "wifi": {
      "mode": "client",
      "routing": "wlan"
  },
  "controls": {
      "failsafeTimeout": 3,
      "throttle": {
          "min": 1000,
          "max": 2000,
          "failsafe": 1500,
          "scaleForward": 100,
          "scaleReverse": 100,
          "idle": 1500
      },
      "steering": {
          "min": 1000,
          "max": 2000,
          "failsafe": 1500,
          "trim": 0,
          "scale": 100,
          "invert": false
      }
  },
  "modem": {
    "path": "/dev/ttyACM0"
  },
  "extras": {
      "samba": false,
      "logLevel": "ERROR"
  }
}