{
  "autostart": {
      "control": false,
      "video": false
  },
  "server": {
      "mode": "direct",
      "host": "192.168.1.100",
      "relayServer": "rendezvous.websium.at:8888",
      "relaySessionId": "test123"
  },
  "network": {
    "ports": {
        "control": 6668,
        "video": 6666,
        "webinterface": 5000
    },
    "wifi": {
        "mode": "client",
        "routing": "wlan"
    },
    "modem": {
      "path": "/dev/ttyACM0",
      "limitBands": false,
      "allowedBands": [1, 3, 4, 7, 8, 17, 20, 28]
    }
  },
  "video": {
      "bitrate": 1800000,
      "framerate": 30,
      "iFramePeriod": 15,
      "height": 720,
      "width": 1280,
      "record": false,
      "recordPath": "/data/recordings",
      "testSource": true,
      "camera": {
        "enableHdr": false
      }
  },
  "controls": {
      "failsafeTimeout": 3,
      "throttle": {
          "min": 1000,
          "max": 2000,
          "failsafe": 1500,
          "scaleForward": 100,
          "scaleReverse": 100,
          "minForward": 0,
          "minReverse": 0,
          "idle": 1500
      },
      "steering": {
          "min": 1000,
          "max": 2000,
          "failsafe": 1500,
          "trim": 0,
          "scale": 100,
          "invert": false
      },
      "gpio": {
          "throttle": 18,
          "steering": 13
      }
  },
  "extras": {
      "samba": false,
      "logLevel": "DEBUG"
  }
}